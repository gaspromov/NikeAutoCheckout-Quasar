{
    "browser_action": {
       "default_popup": "src/auth.html"
    },
    "background": {
      "scripts": [
        "src/shared.js"
      ],
      "persistent": true
    },
    
    "content_scripts": [ {
       "all_frames": true,
       "css": [  ],
       "js": [ "src/filling/fill.js" ],
       "match_about_blank": true,
       "matches": [ "*://gs.nike.com/*", "*://gs-payments.nike.com/*" ],
       "run_at": "document_start"
    }],
    "description": "Autofill for Nike. He fills out all the necessary fields on the Nike checkout.",
    "manifest_version": 2,
    "name": "Nike ACO by Cactus-web",
    "icons": {
       "128": "icons/logo.png"
    },
    "permissions": [ 
      "storage", 
      "tabs",
      "webRequest",
      "webRequestBlocking" 
   ],
    "version": "1.0.0",
    "version_name": "test for QuasarCook",
    "author": "CactusWeb"
}
 